def isPrime(n):   
    if n <= 1:    
        return False
    for i in range (2,n//2 + 1):   
        if n % i ==0:          
            return False       
    return True

n = int(input("Bir sayı giriniz:"))

if isPrime(n) == True:
    print(n, "sayısı bir asal sayıdır.")
else:
    print(n, "sayısı bir asal sayı değildir.")    